{
  "name": "mpg-calculator",
  "version": "6.0.0",
  "type": "module",
  "private": false,
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --reporter=junit --outputFile=junit.xml --coverage.enabled --coverage.provider=istanbul --coverage.all",
    "test:mutation": "npx stryker run",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --format gitlab",
    "build-only": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "html-to-image": "^1.11.11",
    "idb": "^8.0.0",
    "vue": "^3.4.21",
    "vue-markdown-render": "^2.1.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.1",
    "@stryker-mutator/core": "^8.2.6",
    "@stryker-mutator/vitest-runner": "^8.2.6",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-istanbul": "^1.4.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-formatter-gitlab": "^5.1.0",
    "eslint-plugin-vitest": "^0.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "fake-indexeddb": "^5.0.2",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.72.0",
    "typescript": "~5.4.3",
    "vite": "^5.2.7",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7"
  }
}